<div class="col s12 m2 l2"></div>
<div class="col s12 m8 l8"><h3><center>Recent stews</center></h3></div>
<div class="col s12 m2 l2"></div> 

{{#each this}}
		<div class="row" >
 				<div class="col s12 m2 l2"></div>
		        <div class="col s12 m8 l8">
		        
			        <div class="card-content valign center-block">
				          <div class="card small">
				           
				            <div class="card-image">
				              <img class="materialboxed" data-caption="{{this.attributes.title}}" src="{{this.attributes.image}}">
				              <span class="card-title">{{this.attributes.title}}</span>
				            </div>
				            
				            <div class="card-content">
				              <p>{{this.attributes.title}}</p>
				            </div>
				         
				            <div class="card-action">
				              <a href="#">Posted by {{this.attributes.username}}</a>
				            </div>

				          </div>  
		        	</div> 
		        </div>	
		        <div class="col s12 m2 l2"></div> 
			</div>		
{{/each}}


  
<script>

  $(document).ready(function(){
    $('.materialboxed').materialbox();
  });

</script>

