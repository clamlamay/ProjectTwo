{{#each this}}
<div class="container">
	<div class="row" >
        <div class="col s12 m6 l7">
	        <div class="card-content valign center-block">
		          <div class="card">
		           
		            <div class="card-image">
		              <img src="{{this.attributes.image}}">
		              <span class="card-title">{{this.attributes.title}}</span>
		            </div>
		            
		            <div class="card-content">
		              <p>{{this.attributes.title}}</p>
		            </div>
		         
		            <div class="card-action">
		              <a href="#">Posted by {{this.attributes.username}}</a>
		            </div>

		          </div>  
        	</div> 
        </div>	 
	</div>
</div>
{{/each}}
